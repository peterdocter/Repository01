<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ffb7b7b7" android:endColor="#ffdbdbdb" android:angle="270.0" android:centerY="0.3" android:centerColor="#ffbababa" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#80b72031" android:endColor="#a0ce0118" android:angle="270.0" android:centerY="0.3" android:centerColor="#80f3001c" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ffb72031" android:endColor="#ffce0118" android:angle="270.0" android:centerY="0.3" android:centerColor="#fff3001c" />
            </shape>
        </clip>
    </item>
</layer-list>